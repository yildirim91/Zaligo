  $("#Yukari").click(function() {
	  $("html,body").stop().animate({ scrollTop: "0" }, 1000);
  });
});
$(window).scroll(function() {
  var uzunluk = $(document).scrollTop();
  if (uzunluk > 300) $("#Yukari").fadeIn(500);
  else { $("#Yukari").fadeOut(500); }
});
